CREATE TABLE campus (
 Campus_ID NUMBER (11, 0) NOT NULL,
 Campus_Name VARCHAR2(20),
 Address VARCHAR2(35),
CONSTRAINT campus_PK PRIMARY KEY (Campus_ID));

CREATE TABLE program (
 Program_ID NUMBER (11, 0) NOT NULL,
 Name VARCHAR2 (20),
CONSTRAINT program_PK PRIMARY KEY (Program_ID));

CREATE TABLE member (
 Member_ID NUMBER (11, 0) NOT NULL,
 Last_Name VARCHAR2 (20),
CONSTRAINT member_PK PRIMARY KEY (Member_ID));

CREATE TABLE type (
 Type_ID NUMBER (11, 0) NOT NULL,
 Description VARCHAR2 (50),
CONSTRAINT type_PK PRIMARY KEY (Type_ID));
CREATE TABLE campus_qtarget (
Campus_QTarget_ID NUMBER (11, 0) NOT NULL,
Campus_ID NUMBER (11, 0) NOT NULL,
 Year NUMBER (4, 0),
 Quarter NUMBER (10,2),
 Quarter2 NUMBER (10,2),
 Quarter3 NUMBER (10,2),
 Quarter4 NUMBER (10,2),
CONSTRAINT campus_target_PK PRIMARY KEY (Campus_QTarget_ID),
CONSTRAINT campus_target_FK FOREIGN KEY (Campus_ID) REFERENCES
campus(Campus_ID));

CREATE TABLE Program_At_Campus (
 Campus_ID NUMBER (11, 0) NOT NULL,
 Program_ID NUMBER (11, 0) NOT NULL,
CONSTRAINT Program_At_Campus_FK_One FOREIGN KEY (Campus_ID) REFERENCES
campus(Campus_ID),
CONSTRAINT Program_At_Campus_FK_Two FOREIGN KEY (Program_ID) REFERENCES
program(Program_ID));

CREATE TABLE member_program
 (Member_ID NUMBER (11, 0) NOT NULL,
 Program_ID NUMBER (11, 0) NOT NULL,
CONSTRAINT member_program_PK PRIMARY KEY (Member_ID, Program_ID),
CONSTRAINT member_program_FK_one FOREIGN KEY (Member_ID) REFERENCES
member(Member_ID),
CONSTRAINT member_program_FK_two FOREIGN KEY (Program_ID) REFERENCES
program(Program_ID));

CREATE TABLE member_target (
 Member_Target_ID NUMBER (11, 0) NOT NULL,
 Member_ID NUMBER (11, 0) NOT NULL,
 Year NUMBER (4, 0),
 Quarter NUMBER (10,2),
 Quarter2 NUMBER (10,2),
 Quarter3 NUMBER (10,2),
 Quarter4 NUMBER (10,2),
CONSTRAINT member_target_PK PRIMARY KEY (Member_Target_ID),
CONSTRAINT member_target_FK FOREIGN KEY (Member_ID) REFERENCES
member(Member_ID));

CREATE TABLE donor (
 Donor_ID NUMBER (11, 0) NOT NULL,
 Fullname VARCHAR2 (45),
 Type_ID NUMBER (11, 0) NOT NULL,
CONSTRAINT donor_PK PRIMARY KEY (Donor_ID),
CONSTRAINT donor_FK FOREIGN KEY (Type_ID) REFERENCES type(Type_ID));

CREATE TABLE member_donor (
 Member_ID NUMBER (11, 0) NOT NULL,
 Donor_ID NUMBER (11, 0) NOT NULL,
CONSTRAINT member_donor_FK_one FOREIGN KEY (Member_ID) REFERENCES
member(Member_ID),
CONSTRAINT member_donor_FK_two FOREIGN KEY (Donor_ID) REFERENCES
donor(Donor_ID));

CREATE TABLE projection (
 Projection_ID NUMBER (11, 0) NOT NULL,
 Donor_ID NUMBER (11, 0) NOT NULL ,
 Year NUMBER (4, 0),
CONSTRAINT projection_PK PRIMARY KEY (Projection_ID),
CONSTRAINT projection_FK FOREIGN KEY (Donor_ID) REFERENCES donor(Donor_ID));

CREATE TABLE donation (
 Donation_ID NUMBER (11, 0) NOT NULL,
 Donation_Date DATE,
 Donor_ID NUMBER (11, 0) NOT NULL,
 Amount NUMBER (10, 2),
 Program_ID NUMBER (11, 0) NOT NULL,
 Member_ID NUMBER (11, 0) NOT NULL,
CONSTRAINT donation_PK PRIMARY KEY (Donation_ID),
CONSTRAINT donation_FK_one FOREIGN KEY(Donor_ID) REFERENCES donor(Donor_ID),
CONSTRAINT donation_FK_two FOREIGN KEY(Program_ID) REFERENCES program(Program_ID),
CONSTRAINT donation_FK_three FOREIGN KEY (Member_ID) REFERENCES
member(Member_ID));

CREATE TABLE member_type (
 Member_ID NUMBER (11, 0) NOT NULL,
 Type_ID NUMBER (11, 0) NOT NULL,
CONSTRAINT member_type_FK_one FOREIGN KEY (Member_ID) REFERENCES
member(Member_ID),
CONSTRAINT member_type_FK_two FOREIGN KEY (Type_ID) REFERENCES type(Type_ID));

CREATE TABLE type_target (
 Type_Target_ID NUMBER (11, 0) NOT NULL,
 Type_ID NUMBER (11, 0) NOT NULL,
 Year NUMBER (4, 0),
 Quarter NUMBER (10,2),
 Quarter2 NUMBER (10,2),
 Quarter3 NUMBER (10,2),
 Quarter4 NUMBER (10,2),
CONSTRAINT type_target_PK PRIMARY KEY (Type_Target_ID),
CONSTRAINT type_target_FK FOREIGN KEY (Type_ID) REFERENCES type(Type_ID));

